class Solution {
    public int maxFrequencyElements(int[] nums) {
       HashMap<Integer,Integer> d=new HashMap<>();
       for(int v: nums){
           d.put(v,d.getOrDefault(v,0)+1);
       }
       int m=0;
       for(Map.Entry<Integer,Integer> v:d.entrySet()){
           if(m<v.getValue()) m=v.getValue();
       }
       int c=0;
       for(Map.Entry<Integer,Integer> v:d.entrySet()){
           if(m==v.getValue()) c+=m;
       }
       return c;
    }
}
